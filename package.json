{
  "name": "react-g",
  "version": "1.3.1",
  "description": "Simple and powerful boilerplate for react projects",
  "main": "src/main.jsx",
  "repository": "https://github.com/Geebrox/react-g.git",
  "author": "geebrox",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server",
    "build": "cross-env NODE_ENV=production webpack",
    "cy:cache": "cypress cache path && cypress cache list && cypress verify",
    "cy:dev": "cross-env NODE_ENV=test webpack-dev-server",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:report": "nyc report --reporter=text --reporter=text-summary",
    "cy:ci": "concurrently -k \"yarn cy:dev\" \"yarn cy:run\" -s first",
    "lint:es": "eslint ./src/**/*.jsx",
    "lint:fix:es": "eslint --fix ./src/**/*.jsx"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(js|jsx)": [
      "npm run lint:fix:es",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "antd": "^4.9.1",
    "core-js": "^3.4.1",
    "i18next": "^19.8.4",
    "mobx": "^6.0.4",
    "mobx-react": "^7.0.5",
    "mobx-react-devtools": "^6.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-fade-in": "^1.1.1",
    "react-i18next": "^11.7.4",
    "react-router-dom": "^5.2.0",
    "regenerator-runtime": "^0.13.3",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@cypress/code-coverage": "^1.10.4",
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "autoprefixer": "^9.7.1",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^5.2.0",
    "babel-plugin-object-to-json-parse": "^0.0.9",
    "babel-plugin-styled-components": "^1.12.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.0.2",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "cypress": "^3.8.0",
    "dotenv": "^8.2.0",
    "error-overlay-webpack-plugin": "^0.4.1",
    "eslint": "^7.14.0",
    "eslint-plugin-cypress": "^2.8.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-webpack-plugin": "^2.4.1",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "image-minimizer-webpack-plugin": "^1.0.0",
    "image-webpack-loader": "^7.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^8.0.0",
    "istanbul-lib-coverage": "^2.0.5",
    "js-cookie": "^2.2.1",
    "lint-staged": "^9.5.0",
    "mini-css-extract-plugin": "^0.8.0",
